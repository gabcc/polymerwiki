<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:paper='urn:import:com.vaadin.polymer.paper.widget'>
    <g:HTMLPanel>

        <div class="container">
            <div class="row w-row">
                <div class="col-md-12">
                    <g:HTMLPanel ui:field="buttonsContainer">
                        <paper:PaperButton ui:field="buttonAddNewUser" addStyleNames="w-btn-primary">Add new user</paper:PaperButton>
                        <!--<paper:PaperButton ui:field="buttonDisableSelected" addStyleNames="w-btn-error">Disable selected user</paper:PaperButton>-->
                    </g:HTMLPanel>
                </div>
            </div>

            <div class="row w-row">
                <div class="col-md-12">
                    <g:SimplePanel ui:field="celltableHolder"></g:SimplePanel>
                </div>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>